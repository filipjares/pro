<Group labelreference="L24" drawlabel="true">
<Input>
<Text-field style="Normal" layout="Normal">Buchberger's algorithm</Text-field>
</Input>
</Group>
<Group labelreference="L25" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">BuchGB := proc(F,mo)
    local s, G, G2, addedSomething, i, j, k, alreadyIn, p, q, div, r;
    G := F;
    do
        G2 := G;
        addedSomething := false;
        
        for i from 1 to nops(G2) do
            for j from 1 to nops(G2) do
                if i = j then next; end if;
                p := G2[i];
                q := G2[j];
                s := SPol(p,q, mo);
                div := PolyDiv(s, G2, mo);
                r := div[1];
                if r &lt;&gt; 0 then
                    alreadyIn := false;

                    # is r already in G?
                    for k from 1 to nops(G) do
                        if G[k] = r then
                            alreadyIn := true;
                            break;
                        end if;
                    end do;

</Font><Font size="12">                       # add r to G only if it is not already there</Font><Font size="14">
                    if (alreadyIn = false) then
                        addedSomething := true;
                        G := [op(G), r];
                    end if;
                end if;
            end do;
        end do;
        
        if (addedSomething = false) then
            break
        end if;
    end do;
    G;
end proc:</Font></Text-field>
</Input>
</Group>
