<Group labelreference="L18" drawlabel="true">
<Input>
<Text-field style="Normal" layout="Normal">Division algortithm</Text-field>
</Input>
</Group>
<Group labelreference="L19" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" size="14" layout="Normal"><Font size="12"># Vypocet deleni polynomu f nekolika deliteli (polynomy) usporadanymi v seznamu F s monomialovym usporadanim mo
PolyDiv :=  proc(f,F,mo)
local s,p,r,a,i,divOccured,ltp,ltf;
r := 0;
s := nops(F);
a := [seq(0,i=1..s)];
p := f;
while (p &lt;&gt; 0) do
    i := 1;
    divOccured := false;
    ltp := LT(p,mo);
    while (i &lt;= s and divOccured = false) do
        ltf := LT(F[i],mo);
        if (divide(ltp,ltf)) then
            a := subsop(i = a[i] + ltp/ltf, a);
            p := p - expand(ltp/ltf*F[i]);
            divOccured := true;
        else
            i := i + 1;
        end if;
    end do;
    if (divOccured = false) then
        r := r + ltp;
        p := p - ltp;
    end if;
end do;
[r,a]; # return the list containting the remainder 
       # and the list of coefficients
end proc:</Font></Text-field>
</Input>
</Group>
<Group labelreference="L20" drawlabel="true">
<Input>
<Text-field style="Normal" layout="Normal">Implement S-Polynomial computation</Text-field>
</Input>
</Group>
<Group labelreference="L21" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">SPol:=proc(f,g,mo)
local lmf, lmg, leastCm;
    lmf := </Font><Font size="12">LeadingTerm(f, mo)[2];</Font><Font size="14">    # get the leading monomial
    lmg := LeadingTerm(g, mo)[2]; # get the leading monomial
    leastCm := lcm(lmf, lmg);
    expand(leastCm/LT(f, mo)*f - leastCm/LT(g, mo)*g);
end proc:</Font></Text-field>
</Input>
</Group>
