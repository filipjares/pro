                      [R1*s2-Lz*c3+(Mz*P3-Nz*R3)*s3],
                      [(-c1*s2-s1*P1*c2)*P2+s1*R1*R2-Lx*s3-(Mx*P3-Nx*R3)*c3],
                      [(-s1*s2+c1*P1*c2)*P2-c1*R1*R2-Ly*s3-(My*P3-Ny*R3)*c3],
