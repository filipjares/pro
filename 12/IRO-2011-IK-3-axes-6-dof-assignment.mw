                      [-R1*c2*R2+P1*P2-Mz*R3-Nz*P3],
                      [(c1*c2-s1*P1*s2)*A2+s1*R1*D2+c1*A1+(Mx*R3+Nx*P3)*D3+Lx*A3-Qx],
